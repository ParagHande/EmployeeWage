package com.bridgelabz.linecomparisoncomputation;

public class LineCompariosonComputation {

	public static void main(String[] args) {

		int x1 = 5;
		int y1 = 5;
		int x2 = 10;
		int y2 = 10;
		int x = (x2-x1)*(x2-x1);
		int y = (y2-y1)*(y2-y1);
		int z = x + y;

		double t;  
		double sqrtroot= z/2;  
		
		do{  
			t=sqrtroot;  
			sqrtroot=(t+(z/t))/2;  
		}   
		while((t-sqrtroot)!= 0);  
		
		System.out.println("length = "+ sqrtroot);
		
	}

}
